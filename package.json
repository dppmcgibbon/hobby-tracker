{
  "name": "hobby-db",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "nlint": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "perf:check": "node scripts/performance-check.js",
    "analyze": "ANALYZE=true npm run build",
    "migrate:factions": "tsx scripts/migration/import-factions.ts",
    "migrate:paints": "tsx scripts/migration/import-paints.ts",
    "migrate:miniatures": "tsx scripts/migration/import-miniatures.ts",
    "migrate:all": "npm run migrate:factions && npm run migrate:paints",
    "migrate:images": "tsx scripts/migration/upload-images.ts",
    "migrate:verify": "tsx scripts/migration/verify.ts",
    "migrate:collect": "tsx scripts/migration/generate-collect-migration.ts",
    "backup": "tsx scripts/migration/backup.ts",
    "type-check": "tsc --noEmit",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:push": "supabase db push",
    "supabase:push-data": "./scripts/push-local-to-remote.sh",
    "supabase:sync-storage": "tsx scripts/sync-storage-to-remote.ts",
    "supabase:reset": "supabase db reset",
    "remove-backgrounds": "tsx scripts/remove-backgrounds.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-pdf/renderer": "^4.3.2",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.3",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "jspdf": "^4.1.0",
    "jspdf-autotable": "^5.0.7",
    "jszip": "^3.10.1",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-colorful": "^5.6.1",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3",
    "react-dropzone": "^14.4.0",
    "react-hook-form": "^7.71.1",
    "react-intersection-observer": "^10.0.2",
    "react-select": "^5.10.2",
    "recharts": "^2.15.4",
    "sharp": "^0.34.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tsx": "^4.21.0",
    "use-debounce": "^10.1.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/jszip": "^3.4.0",
    "@types/node": "^20.19.30",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-select": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "csv-parser": "^3.2.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "postcss": "^8.4.49",
    "prettier": "^3.8.1",
    "supabase": "^2.74.5",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
